{
  "name": "tasks-timeline-view",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "Leonezz",
  "license": "MIT",
  "repository": "Leonezz/tasks-timeline-view",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "build:tailwind": "npx tailwindcss -i ./src/styles.module.css -o ./dist/index.css --watch",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "@nextui-org/react": "latest",
    "framer-motion": "^10.16.16",
    "moment": "latest",
    "react": "latest",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@babel/eslint-parser": "latest",
    "@babel/plugin-proposal-private-property-in-object": "latest",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@nextui-org/react": "latest",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "commitlint": "latest",
    "cross-env": "^7.0.2",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-config-standard-react": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "framer-motion": "^10.16.16",
    "gh-pages": "^2.2.0",
    "husky": "^8.0.3",
    "microbundle-crl": "^0.13.10",
    "moment": "latest",
    "npm-run-all": "^4.1.5",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-scripts": "latest",
    "tailwindcss": "latest",
    "tslib": "latest",
    "typescript": "latest"
  },
  "files": [
    "dist"
  ]
}
